select(`Weapon Skill`, `Percent of White Attack`, `Glancing Blow Damage Increase`, everything(), -c(Delta, `Glancing Penalty`))
wepskill_code <- wepskill_tbl %>%
clean_names()
inherit_miss <- wepskill_code %>%
select(weapon_skill, everything(), -c(percent_of_white_attack, glancing_blow_damage_increase)) %>%
pivot_longer(-weapon_skill, names_to = "type") %>%
mutate(value = value * 100)

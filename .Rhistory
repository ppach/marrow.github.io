wepskill_tbl <- tibble::tribble(
~Delta, ~"Glancing Penalty", ~"Miss Probability", ~"Hit Cap",
15,    0.35,       0.08,   0.09,
14,             0.31,       0.078,   0.088,
13,   0.27,       0.076,   0.086,
12,   0.23,       0.074,   0.084,
